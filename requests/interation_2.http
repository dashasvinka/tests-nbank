// Positive test: User can make a deposit into his created account
POST http://localhost:4111/api/v1/accounts/deposit
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "id": 2,
  "balance": 5000
}

// Negative test: User can not make a deposit into his created account more than 5000
POST http://localhost:4111/api/v1/accounts/deposit
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "id": 2,
  "balance": 5000.01
}

// Negative test: User can not make a deposit into his created account value equal to neutral
POST http://localhost:4111/api/v1/accounts/deposit
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "id": 2,
  "balance": 0
}

// Negative test: User can not make a deposit into his created account value equal to negative
POST http://localhost:4111/api/v1/accounts/deposit
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "id": 2,
  "balance": -1
}

// Negative test: User can not make a deposit into his uncreated account
POST http://localhost:4111/api/v1/accounts/deposit
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "id": 22,
  "balance": 1500
}

// Negative test: User can not make a deposit into someone else's account
POST http://localhost:4111/api/v1/accounts/deposit
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "id": 4,
  "balance": 1500
}


// Positive test: User can make a transfer from one of his accounts to another
POST http://localhost:4111/api/v1/accounts/transfer
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "senderAccountId": 2,
  "receiverAccountId": 3,
  "amount": 10000
}

// Negative test: User can not make a transfer over 10000
POST http://localhost:4111/api/v1/accounts/transfer
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "senderAccountId": 2,
  "receiverAccountId": 3,
  "amount": 10000.01
}

// Negative test: User can not make a transfer in excess of the amount he has on deposit
POST http://localhost:4111/api/v1/accounts/transfer
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "senderAccountId": 2,
  "receiverAccountId": 3,
  "amount": 1000
}

// Negative test: User can not make a transfer equal to 0
POST http://localhost:4111/api/v1/accounts/transfer
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "senderAccountId": 2,
  "receiverAccountId": 3,
  "amount": 0
}

// Negative test: User can not make a transfer equal to a negative amount
POST http://localhost:4111/api/v1/accounts/transfer
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "senderAccountId": 2,
  "receiverAccountId": 3,
  "amount": -100
}

