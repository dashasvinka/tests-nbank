// Positive test: User can add a missing name
POST http://localhost:4111/api/v1/customer/profile
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "name": "Dasha Kozina"
}

// Positive test: User can update exist name
POST http://localhost:4111/api/v1/customer/profile
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "name": "Dasha Khmelkova"
}

// Negative test: User can not update exist name to one word name
POST http://localhost:4111/api/v1/customer/profile
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "name": "Kozina"
}

// Negative test: User can not update exist name to a name consisting of two words separated by a character other than a space
POST http://localhost:4111/api/v1/customer/profile
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "name": "Kozina_Daria"
}

// Negative test: User can not updated for a name containing symbols other than letters
POST http://localhost:4111/api/v1/customer/profile
Content-Type: application/json
Authorization: Basic ZGFzaGExOTk1OmRhc2hhMTk5NUQk
{
  "name": "#Kozina Daria1"
}


